State => ({\n      posts: prevState.posts.map(post => {\n        return post.id === id ? updatedPost : post\n      })\n    }))\n  }\n\n  handlePostDelete = async (id) => {\n    await destroyPost(id);\n    this.setState(prevState => ({\n      posts: prevState.posts.filter(post => {\n        return post.id !== id\n      })\n    }))\n  }\n  render() {\n    console.log(this.state.userExists)\n    return (\n      <main>\n        {\n          this.state.userExists ?\n            <>\n\n              <Route exact path=\"/login\">\n                {this.state.userExists ? <Redirect to=\"/\" /> : < LoginPage />}\n              </Route>\n\n              <Route exact path=\"/\" render={(props) => (\n                <HomePage\n                  {...props}\n                  handlePostDelete={this.handlePostDelete}\n                  postList={this.state.posts}\n                />\n              )} />\n\n\n              <Route exact path=\"/profile\" render={(props) => (\n                <ProfilePage\n                  {...props}\n                  currentUser={this.state.userExists}\n                />\n              )} />\n\n\n              <Route exact path=\"/posts/new\" render={(props) => (\n                <CreatePost\n                  {...props}\n                  currentUser={this.props.currentUser}\n                  handlePostSubmit={this.handlePostSubmit}\n                />\n              )} />\n\n              <Route exact path=\"/posts/:id\" render={(props) => {\n                const { id } = props.match.params\n                return <PostInfoPage\n                  {...props}\n                  handlePostDelete={this.handlePostDelete}\n                  postId={id}\n                  currentUser={this.props.currentUser}\n                />\n              }\n              } />\n\n              <Route exact path='/posts/:id/edit' render={(props) => {\n                const { id } = props.match.params\n                return <PostEditPage\n                  {...props}\n                  handlePostUpdate={this.handlePostUpdate}\n                  postId={id}\n                />\n              }} />\n\n\n            </>\n            :\n            <>\n              <Redirect to='/login' />\n              <Route exact path='/login' render={(props) => (\n                <LoginPage\n                  {...props}\n                  handleLogin={this.props.handleLogin}\n                />\n              )} />\n              <Route exact path='/register' render={(props) => (\n                <Register\n                  {...props}\n                  handleRegister={this.props.handleRegister}\n                />\n              )} />\n\n            </>\n        }\n      </main>\n    )\n  }\n}","/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/ProfilePage/ProfilePage.jsx",["115","116"],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/PostInfoPage/PostInfoPage.jsx",["117","118","119","120","121","122"],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/HomePage/HomePage.jsx",["123","124","125"],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/PostEditPage/PostEditPage.jsx",[],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/CreatePost/CreatePost.jsx",["126"],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/HomePage/SideBar.jsx",[],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/HomePage/PostFeedSection.jsx",["127","128"],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/HomePage/CreatePostBox.jsx",[],"/Users/jasoncruz/Documents/clones/social-media-app/client/src/components/LoginPage/LoginPage.jsx",["129","130","131","132","133"],"import React, { Component, useState, useEffect } from 'react'\nimport { Link, Router } from 'react-router-dom'\nimport './LoginPage.css'\nimport Logo from '../../resources/images/post-tree-logo.png'\n\nconst Login = pro